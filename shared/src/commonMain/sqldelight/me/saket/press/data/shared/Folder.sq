import me.saket.press.shared.db.FolderId;

CREATE TABLE folder (
  id TEXT AS FolderId NOT NULL,
  name TEXT NOT NULL,
  parent TEXT AS FolderId
);

foldersUnder:
SELECT * FROM folder
WHERE parent = ?;

folder:
SELECT * FROM folder
WHERE id = ?;

insert:
INSERT INTO folder
VALUES ?;

allFolders:
SELECT * FROM folder;
